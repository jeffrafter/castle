<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Channel</th>
    <th>Region</th>
  </tr>

<% for feed in @feeds %>
  <tr>
    <td><%=h feed.title %></td>
    <td><%=h feed.channel.title %></td>
    <td><%=h feed.channel.region.name %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New feed', new_feed_path %>
